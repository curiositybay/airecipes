APP_NAME=your-app-name

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000

# App Configuration
NEXT_PUBLIC_APP_NAME=Your App Name
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_ENVIRONMENT=development
NEXT_PUBLIC_APP_DESCRIPTION=Your App Description

# Domain and URLs
NEXT_PUBLIC_APP_DOMAIN=your-domain.com
NEXT_PUBLIC_APP_URL=https://your-domain.com
NEXT_PUBLIC_GITHUB_REPO=https://github.com/your-username/your-repo

# SEO and Social Media
NEXT_PUBLIC_APP_AUTHOR=Your Name
NEXT_PUBLIC_APP_KEYWORDS=web development, your app
NEXT_PUBLIC_TWITTER_HANDLE=@yourusername
NEXT_PUBLIC_CONTACT_EMAIL=info@your-domain.com

# Error Messages
NEXT_PUBLIC_404_MESSAGE=The page you're looking for doesn't exist.
NEXT_PUBLIC_500_MESSAGE=Something went wrong on our end. Please try again later.

# Authentication (if using)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-here

# Database Configuration
DB_USER=postgres
DB_PASSWORD=postgres_password
DB_CONTAINER=postgres-dev
DB_PORT=5432
DB_NAME=main
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@${DB_CONTAINER}:${DB_PORT}/${DB_NAME}

# Redis Configuration
REDIS_PASSWORD=your-redis-password
REDIS_CONTAINER=redis-dev
REDIS_PORT=6379
REDIS_URL=redis://:${REDIS_PASSWORD}@${REDIS_CONTAINER}:${REDIS_PORT}

# OpenAI Configuration
OPENAI_API_KEY="your_open_ai_key_here"
# The OpenAI model to use (e.g., gpt-4.1-nano, gpt-4.1-mini, gpt-4.1)
OPENAI_MODEL="gpt-4.1-nano"

